    "def LIS(a):\n",
    "    L = [1]*len(a)\n",
    "    for i in range(1,len(a)):\n",
    "        for j in range(1,i):\n",
    "            if (a[j]<a[i]):\n",
    "                L[i]=max(L[i],(L[j]+1))\n",
    "                print(L)\n",
    "    return L"
   ]
